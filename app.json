{
	"name": "Reverse Proxy",
	"description": "Simple reverse proxy for Heroku",
	"keywords": [
		"reverse proxy",
		"nginx"
	],
	"repository": "https://github.com/funwhilelost/heroku-reverse-proxy",
	"buildpacks": [
		{
			"url": "https://github.com/heroku/heroku-buildpack-nginx"
		}
	],
	"env": {
		"UPSTREAM_SERVER": {
			"main-proxy"
		}
		"UPSTREAM_SERVER2": {
			"serv2"
		}
		"UPSTREAM_SERVER3": {
			"serv3"
		}
		"UPSTREAM_SERVER4": {
			"serv4"
		}

		"SUBDOMAIN1": {
			"serv2subdomain"
		}
		"SUBDOMAIN3": {
			"serv3subdomain"
		}
		"SUBDOMAIN4": {
			"serv4subdomain"
		}
		
		"SUBPATH1": {
			"serv2subpath"
		}
		"DOMAIN": {
			"domain"
		}

	},
	"stack": "heroku-16"
}
